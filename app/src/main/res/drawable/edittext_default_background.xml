<?xml version="1.0" encoding="utf-8"?>
<selector xmlns:android="http://schemas.android.com/apk/res/android">
    <item android:drawable="@drawable/edittext_default_background_normal" android:state_enabled="true" android:state_window_focused="false" />
    <item android:drawable="@drawable/edittext_default_background_normal" android:state_enabled="false" android:state_window_focused="false" />
    <item android:state_pressed="true">
        <nine-patch android:src="@drawable/edittext_default_background_focus" android:tint="@color/tint_normal_default" />
    </item>
    <item android:state_enabled="true" android:state_focused="true">
        <nine-patch android:src="@drawable/edittext_default_background_focus" android:tint="@color/tint_normal_default" />
    </item>
    <item android:drawable="@drawable/edittext_default_background_normal" android:state_enabled="true" />
    <item android:state_focused="true">
        <nine-patch android:src="@drawable/edittext_default_background_focus" android:tint="@color/tint_normal_default" />
    </item>
    <item android:drawable="@drawable/edittext_default_background_normal" />

</selector>
